{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "ESNext"],
    "strict": true,
    "allowJs": false,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "isolatedModules": true,
"types": ["vite/client", "aframe"],
    "baseUrl": ".",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src"]
}
